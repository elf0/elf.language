Point: <T>
  new $ x, y
   .x = x
   .y = y
 
  $ : x, y
   x = .x
   y = .y
 
  $ x, y {
   .x = x
   .y = y
  }
 
  X $>
   return .x
 
  Y $>
   return .y
 
  X $ value
   .x = value
 
  Y $ value
   .y = value
 
  OffsetX $ value
   .x += value
 
  OffsetY $ value
   .y += value
 
  Offset $ x, y {
   .x += x
   .y += y
  }
 
  private:
  x T
  y T
 
