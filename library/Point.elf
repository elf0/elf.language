root: elf

Point: class<T>{
  set(x: T, y: T){
    .x = x
    .y = y
  }

  X: get T{
    return .x
  }

  Y: get T{
    return .y
  }

  X: set T{
    .x = value
  }

  Y: set T{
    .y = value
  }

  OffsetX: set T{
    .x += value
  }

  OffsetY: set T{
    .y += value
  }

  Offset: set(x: T, y: T){
    .x += x
    .y += y
  }

private:
  x: T
  y: T
}

