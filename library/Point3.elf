root: elf.Point3

import{
 Point
}

class<T>(Point<T>){
  set(x: T, y: T, z: T){
    Point(x, y)
    .z = value
  }
  
  Z: get T{
    return .z
  }

  Z: set T{
    .z = value
  }
  
  OffsetZ: set T{
    .z += value
  }
  
  Offset: set(x: T, y: T, z: T){
    Point.Offset(x, y)
    .z += z
  }
private:
  z: T
}
